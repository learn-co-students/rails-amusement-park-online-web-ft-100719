<h1>Sign In</h1> 

<%= form_tag '/signin' do %> 
    <%= label_tag "Your Name:" %>
    <%= select_tag "user_name", options_from_collection_for_select(@users, "name", "name") %><br> 
    <%= label_tag "Your Password:" %>
    <%= password_field_tag :password %><br>

    <%= submit_tag "Sign In" %> 
<% end  %>
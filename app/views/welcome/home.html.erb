<h1>Welcome to Our Amusement Park</h1>
<% if logged_in? %>
    <h2> Hello <%= current_user.name %> </h2>
    <%= link_to "Logout", signout_path %>
    <%= link_to "My Stats", user_path(current_user) %>
<% else %>  
    <%= link_to "Sign In", signin_path %>
    <%= link_to "Sign Up", new_user_path %>  
<%end %> 

